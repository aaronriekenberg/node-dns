{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "*": [
        "types/*"
      ]
    },
    "allowSyntheticDefaultImports": true,
    "lib": [
      "ES2020"
    ],
    "module": "ES2020",
    "moduleResolution": "node",
    "target": "ES2020",
    "outDir": "./built",
    "strict": true
  },
  "include": [
    "./src/**/*"
  ]
}